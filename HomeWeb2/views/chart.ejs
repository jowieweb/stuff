<script type="text/javascript" src="loader.js"></script>


<% include partials/header %>
<% include partials/chartjs %>


<div class="row" style="margin:0px;padding:0px">
	<div class="col-2" style="margin:0px;padding:0px">

		<% trons.forEach(function(value, key) { %>
			<% trons.get(key).forEach(function(subValue, subKey) { %>
				<% for (var gui in subValue){	%>
					<% if (subValue[gui].writeDB[0] == "1") {%>
						 <div>
							<input type="checkbox" id="<%=subValue[gui].id%>" name="feature" value="<%= subValue[gui].titel %>" onclick='cbSelect(this);' />
							<label for="<%=subValue[gui].id%>"><%= subKey %> <%= subValue[gui].titel %></label>
						</div>
		
					<% } %>
						
				<% } %>
			<% }, trons.get(key)); %>
			
			
		<% }, trons); %>
		
	</div>
	
	<div class="col-10" style="margin:0px;padding:0px">
		<div id="chart_div" class="white_content" style="height:'100%'">		
		</div>
		
	</div>
</div>
